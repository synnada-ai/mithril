                    "left": {"name": "input1", "type": "tensor"}, 
                    "right": {"name": "input2", "type": "tensor"}
                },
                "m2": {
                    "left": {"name": "input2", "type": "tensor"}, 
                    "right": {"name": "input3", "type": "tensor"}
                },
                "m3": {
                    "left": {"name": "input1", "type": "tensor"}, 
                    "right": {"name": "input3", "type": "tensor"}, 
                    "output": {
                        "name": "output3", 
                        "expose": true
                    }
                },
                "m4": {
                    "left": {"connect": [["m1", "output"]]}, 
                    "right": {"connect": [["m2","output"]]}, 
                    "output": {
                        "name": "output1", 
                        "expose": true
                    }
                },
                "m5": {
                    "left": {"connect": [["m2", "output"]]}, 
                    "right": {"connect": [["m3","output"]]}, 
                    "output": {
                        "name": "output2", 
                        "expose": true
                    }
                }
            }
        },
        "inputs": {
            "input1": [3.0],
            "input2": [2.0],
            "input3": [1.0]
        },
        "output_grads": {
            "output1": [1.0],
            "output3": [1.0]
        },
        "results": {
            "eval": {
                "output1": [9.0],
                "output2": [9.0],
                "output3": [3.0]
            },
            "grad": {
                "input1": [3.0],
                "input2": [4.0],
                "input3": [4.0]
            } 
        }
    },
